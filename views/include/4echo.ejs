<div class="accordion mt-1" id="accordionEcho4">
    <% data4.forEach((item, index) => { %>
        <div class="accordion-item">
            <h2 class="accordion-header" id="heading<%= index %>">
                <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapse<%= index %>Echo4" aria-expanded="true" aria-controls="collapse<%= index %>">
                    <img src="/assets/heartshekast.png" alt="Icon" style="width: 30px;height: 30px;margin-right: 5px;" class="icon-small"> <%= item.title %>
                </button>
            </h2>
            <div id="collapse<%= index %>Echo4" class="accordion-collapse collapse" aria-labelledby="heading<%= index %>" data-bs-parent="#accordionEcho4">
                <div class="accordion-body">
                    <div class="table-responsive">
                        <table class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <% if (path!='EchoForAll') { %>
                                    <th>Select</th>
                                    <% } %>
                                    <th>Parameter</th>
                                    <th>Inspiration</th>
                                    <th>Expiration</th>
                                    <th>Apnea</th>
                                    <th>User Value</th>
                                    <th>Result</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% item.parameters.forEach(parameter => { %>
                                    <tr>
                                        <% if (path!='EchoForAll') { %>
                                        <td data-label="Select"><input type="checkbox" name="select"></td>
                                        <% } %>
                                        <td data-label="Parameter"><%= parameter.parameter %></td>
                                        <td data-label="Inspiration">Mean ± SD: <%= parameter.Inspiration.mean_sd %></td>
                                        <td data-label="Expiration">Mean ± SD: <%= parameter.Expiration.mean_sd %></td>
                                        <td data-label="Apnea">Mean ± SD: <%= parameter.Apnea.mean_sd %></td>
                                        <td data-label="User Value">
                                            <input type="text" name="userValue" class="form-control">
                                            <% if (path!='EchoForAll') { %>
                                             <input type="checkbox" name="uservalue-select" id="4uservalue-select-<%= index %>">
                                             <label for="4uservalue-select-<%= index %>">select</label>
                                            <% } %> 
                                        </td>
                                        <td data-label="Result" class="result-checkboxes">
                                                <label for="normal-<%= index %>"> <input type="checkbox" id="normal-<%= index %>" name="Normal" value="normal"> Normal</label>
                                                <label for="moderate-<%= index %>"> <input type="checkbox" id="moderate-<%= index %>" name="Moderate" value="moderate" >Moderate</label>
                                                <label for="abnormal-<%= index %>"><input type="checkbox" id="abnormal-<%= index %>" name="Abnormal" value="abnormal" >Abnormal</label>
                                            <% if (path!='EchoForAll') { %>
                                             <label  for="selectResult-<%= index %>"> <input  type="checkbox" name="selectResult" id="selectResult-<%= index %>">select</label>
                                            <% } %> 
                                        </td>
                                    </tr>
                                <% }); %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    <% }); %>
</div>

