<div class="accordion mt-1" id="accordionEcho2">
    <% data2.forEach((item, index) => { %>
        <div class="accordion-item">
            <h2 class="accordion-header" id="heading<%= index %>">
                <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapse<%= index %>echo2" aria-expanded="true" aria-controls="collapse<%= index %>echo2">
                    <img src="/assets/heartusb.png" alt="Icon" style="width: 30px;height: 30px;margin-right: 5px;" class="icon-small"> <%= item.title %>
                </button>
            </h2>
            <div id="collapse<%= index %>echo2" class="accordion-collapse collapse" aria-labelledby="heading<%= index %>echo2" data-bs-parent="#accordionEcho2">
                <div class="accordion-body">
                    <table class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <% if (path!='EchoForAll') { %>
                                    <th>Select</th>
                                <% } %>
                                <th>Parameter</th>
                                <th>Male Range</th>
                                <th>Female Range</th>
                                <th>User Value</th>
                                <th>Result</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% item.parameters.forEach(parameter => { %>
                                <tr>
                                    <% if (path!='EchoForAll') { %>
                                    <td data-label="Select"><input type="checkbox" name="select"></td>
                                    <% } %>
                                    <td data-label="Parameter"><%= parameter.parameter %></td>
                                    <td data-label="Male Range"><%= parameter.male.range %></td>
                                    <td data-label="Female Range"><%= parameter.female.range %></td>
                                    <td data-label="User Value">
                                        <input type="text" name="uservalue" class="form-control">
                                        <% if (path!='EchoForAll') { %>
                                            <input type="checkbox" name="uservalue-select" id="2uservalue-<%= index %>" >
                                            <label for="2uservalue-<%= index %>">select</label>
                                        <% } %>
                                    </td>
                                    <td data-label="Result" class="result-checkboxes">
                                            <label for="normal-<%= index %>">
                                                <input type="checkbox" id="normal-<%= index %>" name="Normal" value="normal"> Normal
                                            </label>
                                            <label for="moderate-<%= index %>">
                                                <input type="checkbox" id="moderate-<%= index %>" name="Moderate" value="moderate"> Moderate
                                            </label>
                                            <label for="abnormal-<%= index %>">
                                                <input type="checkbox" id="abnormal-<%= index %>" name="Abnormal" value="abnormal"> Abnormal
                                            </label>
                                        <% if (path != 'EchoForAll') { %>
                                            <label for="selectResult<%= index %>" >  
                                                <input type="checkbox" name="selectResult"  id="selectResult<%= index %>">select</label>
                                        <% } %>
                                    </td>
                                </tr>
                            <% }) %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    <% }) %>
</div>