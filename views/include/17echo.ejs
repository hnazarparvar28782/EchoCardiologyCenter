<div class="accordion mt-1" id="accordionEcho17">
    <% data17.forEach((item, index) => { %>
        <div class="accordion-item">
            <h2 class="accordion-header" id="heading<%= index %>">
                <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapse<%= index %>echo17" aria-expanded="true" aria-controls="collapse<%= index %>echo17">
                    <img src="/assets/cardiparvand.png" alt="Icon" style="width: 30px;height: 30px;margin-right: 5px;" class="icon-small"> <%= item.title %>
                    <%= item.title %>
                </button>
            </h2>
            <div id="collapse<%= index %>echo17" class="accordion-collapse collapse" aria-labelledby="heading<%= index %>" data-bs-parent="#accordionEcho17">
                <div class="accordion-body">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th>Select</th>
                                <th>Stage</th>
                                <th>Definition</th>
                                <th>Valve Hemodynamics</th>
                                <th>Hemodynamic Consequences</th>
                                <th>Clinical Symptoms</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% item.parameters.forEach(parameter => { %>
                                <tr>
                                    <td><input type="checkbox" name="select"></td>
                                    <td data-label="Stage"><%= parameter.stage %></td>
                                    <td data-label="Definition"><%= parameter.definition %></td>
                                    <td data-label="Valve Hemodynamics"><%= parameter.valve_hemodynamics %></td>
                                    <td data-label="Hemodynamic Consequences" style="font-size: 10px;"><%= parameter.hemodynamic_consequences %></td>
                                    <td data-label="Clinical Symptoms"><%= parameter.clinical_symptoms %></td>
                                </tr>
                            <% }) %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    <% }) %>
</div>